{
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": 0
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "__dirname": false,
    "app": false,
    "page": false,
    "Polymer": false,
    "module": false,
    "process": false,
    "require": false
  }
}
